{
  "private": true,
  "name": "rjhllr/vue3-form-json-schema",
  "version": "1.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "preview": "turbo run preview --parallel",
    "lint": "turbo run lint",
    "build:lib": "pnpm build --filter lib"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "prettier": "^2.6.2",
    "turbo": "^1.2.16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint-plugin-jsx-a11y",
        "eslint-plugin-react-hooks",
        "eslint-plugin-react",
        "rollup"
      ]
    },
    "overrides": {
      "minimist": "^1.2.6"
    }
  },
  "packageManager": "pnpm@6.32.2"
}
